		"@nestjs/core": "^10.3.0",
		"@nestjs/jwt": "^10.2.0",
		"@nestjs/platform-express": "^10.3.7",
		"@nestjs/sequelize": "^10.0.0",
		"@nestjs/swagger": "^7.1.17",
		"@types/multer": "^1.4.11",
		"axios": "^1.6.2",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.0",
