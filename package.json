		"reflect-metadata": "^0.1.12",
		"regression": "^2.0.1",
		"request-ip": "^3.3.0",
		"run": "^1.5.0",
		"rxjs": "^7.8.1",
		"sequelize": "^6.37.5",
		"sequelize-cli": "^6.6.2",
		"sequelize-typescript": "^2.1.6",
		"uuid": "^9.0.1",
		"winston": "^3.11.0",
		"winston-azure-blob": "^1.5.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^18.4.3",
		"@commitlint/config-conventional": "^18.4.3",
		"@nestjs/cli": "^10.2.1",
		"@nestjs/schematics": "^10.0.3",
		"@nestjs/testing": "^10.3.0",
